{
  "compilerOptions": {
    "target": "es2017",
    "outDir": "./build",
    "module": "commonjs",
    "rootDir": ".",
    "types": ["node"],
    "resolveJsonModule": true,
    "diagnostics": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": true
  },
  "include": ["src/**/*.ts", "prisma/*"],
}
